{
  "name": "library-management-typeorm",
  "version": "1.0.0",
  "description": "Library Management System with TypeORM",
  "main": "dist/server.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env IS_TS_NODE=true ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env IS_TS_NODE=true nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register src/server.ts",
    "start:debug": "cross-env IS_TS_NODE=true nodemon --watch 'src/**/*.ts' --exec node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:drop": "npm run typeorm -- schema:drop -d ormconfig.ts",
    "db:create": "node generate-migration.js",
    "db:generate": "npm run typeorm -- migration:generate -n MigrationName -d ormconfig.ts",
    "db:migrate": "npm run typeorm -- migration:run -d ormconfig.ts",
    "db:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
    "db:show": "npm run typeorm -- migration:show -d ormconfig.ts",
    "db:seed": "cross-env IS_TS_NODE=true ts-node -r tsconfig-paths/register src/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/colors": "^1.2.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "chalk": "^5.3.0",
    "concurrently": "^9.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
